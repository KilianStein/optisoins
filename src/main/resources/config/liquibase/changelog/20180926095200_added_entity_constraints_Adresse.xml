<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Adresse.
    -->
    <changeSet id="20180926095200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="orthophoniste_id"
                                 baseTableName="adresse"
                                 constraintName="fk_adresse_orthophoniste_id"
                                 referencedColumnNames="id"
                                 referencedTableName="orthophoniste"/>

        <addForeignKeyConstraint baseColumnNames="patient_id"
                                 baseTableName="adresse"
                                 constraintName="fk_adresse_patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="patient"/>

        <addForeignKeyConstraint baseColumnNames="medecin_id"
                                 baseTableName="adresse"
                                 constraintName="fk_adresse_medecin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="medecin"/>

    </changeSet>
</databaseChangeLog>
